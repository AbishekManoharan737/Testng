<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Parallel Test Suite" parallel="classes"
	thread-count="5">

	<test name="Chrome Test">
		<parameter name="browser" value="chrome" />
		<parameter name="version" value="88.0" />
		<parameter name="platform" value="WIN10" />
		<classes>
			<class name="lambdatestjava.LambdaTestJava" />
			<class name="lambdatestjava.LambdaTestJava2" />
			<class name="lambdatestjava.LambdaTestJava3" />
		</classes>
	</test>
	<test name="Firefox Test">
		<parameter name="browser" value="firefox" />
		<parameter name="version" value="82.0" />
		<parameter name="platform" value="WIN7" />
		<classes>
			<class name="lambdatestjava.LambdaTestJava" />
			<class name="lambdatestjava.LambdaTestJava2" />
			<class name="lambdatestjava.LambdaTestJava3" />
		</classes>
	</test>
	<test name="Microsoft Edge Test">
		<parameter name="browser" value="edge" />
		<parameter name="version" value="87.0" />
		<parameter name="platform" value="macOS Sierra" />
		<classes>
			<class name="lambdatestjava.LambdaTestJava" />
			<class name="lambdatestjava.LambdaTestJava2" />
			<class name="lambdatestjava.LambdaTestJava3" />
		</classes>
	</test>
	<test name="IE Test">
		<parameter name="browser" value="ie" />
		<parameter name="version" value="11.0" />
		<parameter name="platform" value="WIN10" />
		<classes>
			<class name="lambdatestjava.LambdaTestJava" />
			<class name="lambdatestjava.LambdaTestJava2" />
			<class name="lambdatestjava.LambdaTestJava3" />
		</classes>
